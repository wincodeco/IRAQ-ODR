<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_company_form_inherit_l10n_ua_partner_legal" model="ir.ui.view">
        <field name="name">view.company.form.inherit.l10n.ar.partner.legal</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_registry']" position="before">
                <field name="registration_certificate"/>
                <field name="taxpayer_status_id"/>
                <field name="director"/>
            </xpath>
        </field>
    </record>

</odoo>
